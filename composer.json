{
  "name": "webgrip/semver-actions",
  "type": "project",
  "authors": [
    {
      "name": "Ryan",
      "email": "R.Grippeling@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "psr/http-message": "^2.0",
    "vlucas/phpdotenv": "5.6.x-dev",
    "guzzlehttp/guzzle": "^7.8",
    "symfony/http-foundation": "^7.0",
    "php-di/php-di": "^7.0",
    "filp/whoops": "^2.15"
  },
  "require-dev": {
    "deployer/deployer": "^7.3",
    "enlightn/security-checker": "^2.0",
    "ergebnis/composer-normalize": "^2.42",
    "phpmd/phpmd": "^2.15",
    "phpspec/prophecy": "^1.19",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "*",
    "rector/rector": "^1.0",
    "squizlabs/php_codesniffer": "^3.9",
    "vimeo/psalm": "^6.0@dev",
    "beberlei/assert": "^3.3",
    "jangregor/phpstan-prophecy": "^1.0",
    "thecodingmachine/phpstan-strict-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.3",
    "php-mock/php-mock-prophecy": "^0.1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Vendic\\Monitoring\\OhDear\\": "src/",
      "Vendic\\Monitoring\\OhDear\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 4,
      "indent-style": "space"
    }
  }
}
